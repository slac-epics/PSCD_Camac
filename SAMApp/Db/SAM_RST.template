record(bo, "$(MODULENAME):Reset") {
  field(DESC, "SAM Reset")
  field(DTYP, "SAM")
  field(OUT,  "#B0 C$(CRATE) N$(SLOT) A0 F0@RESET")
  field(ZNAM, "Reset")
  field(ONAM, "Reset")
}

record(ai, "$(MODULENAME):fwVer") {
  field(DESC, "SAM FW")
  field(SCAN, "$(SCAN)")
  field(DTYP, "SAM")
  field(INP,  "#B0 C$(CRATE) N$(SLOT) A0 F0@VERSION")
  field(PREC, "3")
}


