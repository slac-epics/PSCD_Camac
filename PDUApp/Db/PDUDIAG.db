record(waveform,"$(NAME):DATA") {
  field(DESC,"Dagnostic data")
  field(SCAN, "Passive")
  field(DTYP,"PDUDIAG")
  field(FTVL,"ULONG")
  field(NELM,"5000")
  field(PINI,"YES")
}

record(longout,"$(NAME):CRATE_SELECT") {
  field(DESC, "Crate for PDU")
  field(SCAN, "Passive")
  field(DTYP, "PDUDIAG")
  field(DRVH, "$(DRVH)")
  field(DRVL, "2")
  field(LOPR, "2")
  field(HOPR, "$(DRVH)")
  field(OMSL, "supervisory")
  field(OUT,  "#B0 C0 N0 A0 F0@CRATE")
  field(PINI,"YES")
}

record(longout,"$(NAME):CHAN_SELECT") {
  field(DESC, "Channel for PDU")
  field(SCAN, "Passive")
  field(DTYP, "PDUDIAG")
  field(DRVH, "15")
  field(DRVH, "15")
  field(DRVL, "0")
  field(LOPR, "0")
  field(OMSL, "supervisory")
  field(OUT,  "#B0 C0 N0 A0 F0@CHANL")
  field(PINI,"YES")
}





